{
  "manifest_version": 3,
  "name": "Date Converter",
  "version": "1.0",
  "description": "Convert selected date and show popup and clip",
  "permissions": ["contextMenus", "scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}

